Proceso PositivoNegativoCero
	Definir Num Como Real;
	Escribir "ingrese un numero";
	Leer Num;
	Si Num = 0 Entonces
		Escribir "El numero ingresado es cero";
	FinSi
	Si Num>0 Entonces
		Escribir "El numero ingresado es positivo";
	FinSi
	si Num<0 Entonces
		Escribir "El numero ingresado es negativo";
	FinSi
FinProceso
